#! /bin/sh

# @(#) $Header: /home/deyke/tmp/cvs/tcp/tools/vixxx,v 1.2 1993-02-23 21:33:48 deyke Exp $

tmp=/tmp/tmp$$
sed <xxx >$tmp -e "
  / no change$/d
  /^cp: cannot create/d
  /^mv: /d
  /^chmod: can/d
  /^Make:  Don't know how to make /d
  /^ln: .* are identical/d
  /^rm: .* not removed.  Permission denied/d
  / is up to date/d
"
vi $tmp
rm -f $tmp
