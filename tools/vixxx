#! /bin/sh

# @(#) $Header: /home/deyke/tmp/cvs/tcp/tools/vixxx,v 1.1 1993-01-29 06:52:55 deyke Exp $

tmp=/tmp/tmp$$
sed <xxx >$tmp -e "
  / no change$/d
  /Pointers are not assignment-compatible/d
  / is not the correct type/d
  /: Illegal pointer combination for /d
  /^cp: cannot create/d
  /^mv: /d
  /^chmod: can/d
  /^Make:  Don't know how to make /d
  /^ln: .* are identical/d
  /^rm: .* not removed.  Permission denied/d
  / is up to date/d
"
vi $tmp
rm -f $tmp
