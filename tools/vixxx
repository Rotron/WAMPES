#! /bin/sh

# @(#) $Id: vixxx,v 1.10 1999-01-24 22:24:52 deyke Exp $

perl -ne < xxx "

	next if ! /:/;

	next if / .ignored/;
	next if / ERROR in line 15: unknown . sequence/;
	next if / Not owner/;
	next if / Permission denied/;
	next if / are identical/;
	next if / is up to date/;
	next if /: In function /;
	next if /In file included from /;
	next if /ing directory /;
	next if /linux_include.netinet/;
	next if /of .accept. differ in signedness/;
	next if /of .recvfrom. differ in signedness/;

	print;

" | less
