#! /bin/sh

# @(#) $Id: vixxx,v 1.9 1996-08-12 18:52:58 deyke Exp $

tmp=/tmp/tmp$$
sed <xxx >$tmp -e "

	/ change in ANSI C; /d
	/ done$/d
	/ Error code .* (ignored)/d
	/ Error exit code .* (ignored)/d
	/ is up to date/d
	/ no change$/d
	/ warning: macro replacement within a character constant/d
	/acc /d
	/This value will be ignored/d
	/^Cannot create /d
	/^cc /d
	/^chmod[: ]/d
	/^cp[: ]/d
	/^ln[: ]/d
	/^make: /d
	/^mv[: ]/d
	/^Permission denied/d
	/^rm[: ]/d
	/^The error code from the last failed command is /d

	/\
	c89 /d

	/\
	cc /d

"
vi $tmp
rm -f $tmp
