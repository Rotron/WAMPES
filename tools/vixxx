#! /bin/sh

# @(#) $Header: /home/deyke/tmp/cvs/tcp/tools/vixxx,v 1.5 1994-01-21 11:11:18 deyke Exp $

tmp=/tmp/tmp$$
sed <xxx >$tmp -e "
  / Error code .* (ignored)/d
  / change in ANSI C; /d
  / done$/d
  / is up to date/d
  / no change$/d
  / warning: initializer does not fit: /d
  / warning: macro replacement within a character constant/d
  /This value will be ignored/d
  /^Permission denied/d
  /^The error code from the last failed command is /d
  /^chmod[: ]/d
  /^cp[: ]/d
  /^ln[: ]/d
  /^mv[: ]/d
  /^rm[: ]/d
"
vi $tmp
rm -f $tmp
