#! /bin/sh

# @(#) $Header: /home/deyke/tmp/cvs/tcp/tools/vixxx,v 1.6 1995-12-20 09:47:02 deyke Exp $

tmp=/tmp/tmp$$
sed <xxx >$tmp -e "

	/ change in ANSI C; /d
	/ done$/d
	/ Error code .* (ignored)/d
	/ is up to date/d
	/ no change$/d
	/ warning: macro replacement within a character constant/d
	/acc /d
	/This value will be ignored/d
	/^cc /d
	/^chmod[: ]/d
	/^cp[: ]/d
	/^ln[: ]/d
	/^mv[: ]/d
	/^Permission denied/d
	/^rm[: ]/d
	/^The error code from the last failed command is /d

	/\
	c89 /d

	/\
	cc /d

"
vi $tmp
rm -f $tmp
