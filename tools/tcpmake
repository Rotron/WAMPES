#! /bin/csh -f

# @(#) $Header: /home/deyke/tmp/cvs/tcp/tools/tcpmake,v 1.4 1993-06-06 08:24:10 deyke Exp $

foreach dir (           \
	~/tcp           \
	~/tcp/bbs       \
	~/tcp/convers   \
	~/tcp/doc       \
	~/tcp/src       \
	~/tcp/tools     \
	~/tcp/util      )
  cd $dir
  trimfiles
end
tcpremove
cd ~/tcp/src
makeall |& tee xxx
cd ~/tcp
make
mk make
