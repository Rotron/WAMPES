#! /bin/ksh

# @(#) $Header: /home/deyke/tmp/cvs/tcp/tools/print,v 1.4 1995-03-13 13:32:22 deyke Exp $

if [[ -n "$1" ]]; then
	files="setup $*"
else
	files="$(< filelist)"
fi

case ${0##*/} in
nprint)
	cat $files | tbl -TX | nroff -mm | col
	;;
tprint)
	cat $files | tbl | troff -mm
	;;
gnprint)
	cat $files | gtbl | gnroff -mgm
	;;
gtprint)
	cat $files | gtbl | groff -mgm
	;;
*)
	echo $0 not recognized
	exit 1
	;;
esac
