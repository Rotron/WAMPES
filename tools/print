#! /bin/ksh

# @(#) $Header: /home/deyke/tmp/cvs/tcp/tools/print,v 1.2 1994-08-05 10:35:36 deyke Exp $

if [[ -n "$1" ]] ; then
	files="$*"
else
	files="$(< filelist)"
fi

case ${0##*/} in
nprint)
	cat $files | tbl -TX | nroff -mm | col
	;;
tprint)
	cat $files | tbl | troff -mm
	;;
gnprint)
	cat $files | gtbl | gnroff -mgm
	;;
gtprint)
	cat $files | gtbl | groff -mgm
	;;
*)
	echo $0 not recognized
	exit 1
	;;
esac
