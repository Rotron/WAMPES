#! /bin/ksh

# @(#) $Id: print,v 1.7 1996-08-12 18:52:58 deyke Exp $

if [[ -n "$1" ]]; then
	files="setup $*"
else
	files="$(< filelist)"
fi

case ${0##*/} in
nprint)
	cat $files | tbl -TX | nroff -mm | col
	;;
tprint)
	cat $files | tbl | troff -mm
	;;
aprint)
	cat $files | groff -p -t -mgm -Tascii
	;;
pprint)
	cat $files | groff -p -t -mgm -Tps
	;;
*)
	echo $0 not recognized
	exit 1
	;;
esac
