source /tcp/netrom_links

# netrom broadcast axip nodes

echo accept
icmp trace on
ip rtimer 300
ip ttl 30
log /tcp/net.log
tcp irtt 60000
tcp mss 216
tcp window 2048

start discard
start domain
start echo
start ftp
start tcpgate convers unix:/tcp/sockets/convers
start tcpgate finger
start time

sntp add dl1sbl-1

source /tcp/net.rc.local
