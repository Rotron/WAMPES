# @(#) $Id: net.rc.df9sr-1,v 1.22 1999-02-11 19:26:34 deyke Exp $

hostname     df9sr-1
ax25 mycall  df9sr-1
ip address   df9sr-1
netrom ident "#BOEB5"

attach axip                     # ax.25 over host ip
attach ipip                     # ip over host ip
attach netrom                   # ip over netrom

ax25 route add axip default

axip route add dl1sbl-1 15.136.125.16           # hpbblba.bbn.hp.com

route add  44.130.0.0/16        ipip     15.136.125.16
route add  44.143.0.0/16        ipip     15.136.125.16
route add  db0sao-u.ampr.org    ipip     15.136.125.16
route add  db0sao.ampr.org      ipip     15.136.125.16
route add  df3du-1.ampr.org     ipip     15.136.125.16
route add  dh8saw-1.ampr.org    ipip     15.136.125.16
route add  dj3eg-1.ampr.org     ipip     15.136.125.16
route add  dk0hu-u.ampr.org     ipip     15.136.125.16
route add  dk0hu.ampr.org       ipip     15.136.125.16
route add  dl1sbl-1.ampr.org    ipip     15.136.125.16
route add  dl4sas-1.ampr.org    ipip     15.136.125.16
route add  dl4sel-1.ampr.org    ipip     15.136.125.16
route add  dl4sel.ampr.org      ipip     15.136.125.16

source /tcp/netrom_links

# netrom broadcast axip nodes

echo accept
icmp trace on
ip rtimer 300
ip ttl 30
log /tcp/net.log
tcp irtt 60000
tcp mss 216
tcp window 2048

start discard
start domain
start echo
start ftp
start tcpgate convers unix:/tcp/sockets/convers
start tcpgate finger
start time

source /tcp/net.rc.local
