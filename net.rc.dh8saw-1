# @(#) $Header: /home/deyke/tmp/cvs/tcp/Attic/net.rc.dh8saw-1,v 1.17 1994-08-21 10:16:51 deyke Exp $

hostname     dh8saw-1
ax25 mycall  dh8saw-1
ip address   dh8saw-1
netrom ident "#boeb8"

attach axip                     # ax.25 over host ip
attach ipip                     # ip over host ip
attach netrom                   # ip over netrom

ax25 route add axip default

axip route add dk0hu 15.138.33.250

route add  db0sao-u.ampr.org    ipip     15.138.33.250
route add  db0sao.ampr.org      ipip     15.138.33.250
route add  df1tl-1.ampr.org     ipip     15.138.33.250
route add  df1tl-u.ampr.org     ipip     15.138.33.250
route add  df3du-1.ampr.org     ipip     15.138.33.250
route add  df9sr-1.ampr.org     ipip     15.138.33.250
route add  dj3eg-1.ampr.org     ipip     15.138.33.250
route add  dk0hu-u.ampr.org     ipip     15.138.33.250
route add  dk0hu.ampr.org       ipip     15.138.33.250
route add  dk5sg-u.ampr.org     ipip     15.138.33.250
route add  dk5sg.ampr.org       ipip     15.138.33.250
route add  dl1sbl-1.ampr.org    ipip     15.138.33.250
route add  dl2sel-1.ampr.org    ipip     15.138.33.250
route add  dl2sel.ampr.org      ipip     15.138.33.250
route add  dl4sas-1.ampr.org    ipip     15.138.33.250
route add  dl4sel-1.ampr.org    ipip     15.138.33.250
route add  dl4sel.ampr.org      ipip     15.138.33.250

source /tcp/netrom_links

# netrom broadcast axip nodes

echo accept
icmp trace on
ip rtimer 300
ip ttl 30
log /tcp/net.log
tcp irtt 60000
tcp mss 216
tcp window 2048

start discard
start domain
start echo
start ftp
start tcpgate convers unix:/tcp/sockets/convers
start tcpgate finger
start time

source /tcp/net.rc.local
