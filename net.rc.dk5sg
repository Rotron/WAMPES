# @(#) $Header: /home/deyke/tmp/cvs/tcp/net.rc.dk5sg,v 1.35 1994-12-02 21:36:08 deyke Exp $

hostname     dk5sg
ax25 mycall  dk5sg
ip address   dk5sg
netrom ident "#fc"

attach axip                     # ax.25 over host ip
attach ipip                     # ip over host ip
attach ipip ipip94 ip 94        # ip over host ip
attach netrom                   # ip over netrom
attach ni ni dk5sg-u            # ip to host ip

ax25 route add axip default

axip route add db0fho   db0fho
axip route add dl1sbl-1 15.136.120.111          # hpbblb.bbn.hp.com

route add  44.130.0.0/16        ipip     15.136.120.111
route add  44.143.0.0/16        ipip     15.136.120.111
route add  db0fho.ampr.org      ipip     15.255.240.16
route add  db0sao-u.ampr.org    ipip     15.136.120.111
route add  db0sao.ampr.org      ipip     15.136.120.111
route add  df3du-1.ampr.org     ipip     15.136.120.111
route add  df9sr-1.ampr.org     ipip     15.136.120.111
route add  dh8saw-1.ampr.org    ipip     15.136.120.111
route add  dj3eg-1.ampr.org     ipip     15.136.120.111
route add  dk0hu-u.ampr.org     ipip     15.136.120.111
route add  dk0hu.ampr.org       ipip     15.136.120.111
route add  dk5sg-u.ampr.org     ni
route add  dl1sbl-1.ampr.org    ipip     15.136.120.111
route add  dl2sel-1.ampr.org    ipip     15.136.120.111
route add  dl2sel.ampr.org      ipip     15.136.120.111
route add  dl4sas-1.ampr.org    ipip     15.136.120.111
route add  dl4sel-1.ampr.org    ipip     15.136.120.111
route add  dl4sel.ampr.org      ipip     15.136.120.111
route addp 15.0.0.0/8           ni
route addp 44.0.0.0/8           ipip     15.255.240.16
route addp hpcsos.ampr.org      ipip     15.255.240.16

source /tcp/netrom_links

netrom broadcast axip nodes

# flexnet link add dc1ik
flexnet link add dl1sbl-1

domain usegethostby on
echo accept
icmp trace on
ip rtimer 300
ip ttl 30
log /tcp/net.log
tcp irtt 60000
tcp mss 216
tcp window 2048

start discard
start domain
start echo
start ftp
start sntp
start tcpgate convers unix:/tcp/sockets/convers
start tcpgate finger
start tcpgate netupds
start tcpgate nntp
start tcpgate smtp
start time

source /tcp/net.rc.local
